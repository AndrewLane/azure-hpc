{
    "batch_shipyard": {
        "storage_account_settings": "mystorageaccount"
    },
	"docker_registry": {
        "private": {
            "server": "<your docker>.azurecr.io"
        }
    },
    "global_resources": {
        "docker_images": [
            "azuregenomics"
        ],
    	"docker_volumes": {
			"shared_data_volumes": {
				"azurefilevol": {
					"volume_driver": "azurefile",
					"storage_account_settings": "mystorageaccount",
					"azure_file_share_name": "fileshare",
					"container_path": "/path/setup/inthemapping",
					"mount_options": [
						"filemode=0777",
						"dirmode=0777"
					]
				}
            }
        }
	}
}
